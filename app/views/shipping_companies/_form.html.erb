<%= form_with(model: @shipping_company) do |f| %>
    <div>
      <%= f.label :brand_name %>
      <%= f.text_field :brand_name %>
    </div>
    <div>
      <%= f.label :corporate_name %>
      <%= f.text_field :corporate_name %>
    </div>
    <div>
      <%= f.label :registration_number %>
      <%= f.number_field :registration_number %>
    </div>
    <div>
      <%= f.label :email_domain %>
      <%= f.text_field :email_domain %>
    </div>
    <div>
      <%= f.label :address %>
      <%= f.text_field :address %>
    </div>
    <div>
      <%= f.label :city %>
      <%= f.text_field :city %>
    </div>
    <div>
      <%= f.label :state %>
      <%= f.text_field :state %>
    </div>
    <div>
      <%= f.label :postal_code %>
      <%= f.number_field :postal_code %>
    </div>
    <div>
      <%= f.submit 'ENVIAR' %>
    </div>
  <% end %>